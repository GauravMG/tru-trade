# Ignore the .env file which contains environment-specific settings
.env

# Ignore logs and cache files
writable/logs/*
writable/cache/*
writable/debugbar/*
writable/session/*

# Ignore the coverage report generated by PHPUnit
tests/_output/
tests/_support/_generated/

# Composer vendor directory
vendor/

# Node modules directory
node_modules/

# IDE and editor files
*.sublime-project
*.sublime-workspace
.vscode/
.idea/
nbproject/

# Operating system files
.DS_Store
Thumbs.db

# CodeIgniter's generated files
public/uploads/*

# Ignore the PHPUnit result cache
.phpunit.result.cache

# Exclude built assets if using a build tool
public/assets/build/
public/assets/*.map
